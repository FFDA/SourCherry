    <string name="navigation_drawer_button_bookmarks_content_description">Button BOOKMARKS. Displayed in normal and bookmark modes. When pressed - switch menu between normal and bookmark menus.</string>
    <string name="imported_databases_item_export_button_description">EXPORT DATABASE. Displayed next to imported database name. When pressed opens a dialog window for user to choose to encrypt with password and confirm of export operation.</string>
